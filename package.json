{
  "name": "@autoschedule/react-elements",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ava": "ava build/**/*.test.js",
    "test": "tslint -p ./ && tsc && nyc --reporter=lcov npm run ava",
    "test:codecov": "tslint -p ./ && tsc && nyc npm run ava",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts && npm run build:ts -- -m es6 --outDir es",
    "prepublishOnly": "npm run build",
    "doc": "npm run build && typedoc --out docs/ es/",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "watch:ts": "tsc -w",
    "watch:ava": "ava -w build/**/*.test.js",
    "watch:test": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Ava\" -c \"blue.bold,magenta.bold\" \"npm run watch:ts\" \"npm run watch:ava\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AutoScheduleJS/react-elements.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AutoScheduleJS/react-elements/issues"
  },
  "homepage": "https://github.com/AutoScheduleJS/react-elements#readme",
  "peerDependencies": {
    "emotion": "^10.0.5",
    "emotion-theming": "^10.0.5",
    "react": "^16.7.0"
  },
  "devDependencies": {
    "@storybook/addon-info": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.17",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "emotion": "^10.0.5",
    "emotion-theming": "^10.0.5",
    "jest": "^23.6.0",
    "react": "^16.7.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  }
}
